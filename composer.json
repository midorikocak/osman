{
    "name": "midorikocak/osman",
    "type": "library",
    "description": "A modern PHP framework for the future",
    "keywords": [
        "midorikocak",
        "osman"
    ],
    "homepage": "https://github.com/midorikocak/osman",
    "license": "MIT",
    "authors": [
        {
            "name": "Midori Kocak",
            "email": "mtkocak@gmail.com",
            "homepage": "https://midorikocak.github.io/me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "fig/http-message-util": "^1.1",
        "fzaninotto/faker": "^1.8",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/link": "^1.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "midorikocak\\osman\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "midorikocak\\osman\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
